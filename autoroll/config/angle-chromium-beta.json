// See https://skia.googlesource.com/buildbot.git/+show/master/autoroll/go/roller/config.go#130
// for documentation of the autoroller config.
{
  "childDisplayName": "ANGLE",
  "commitMsg": {
    "bugProject": "chromium",
    "childLogURLTmpl": "https://chromium.googlesource.com/angle/angle.git/+log/{{.RollingFrom}}..{{.RollingTo}}",
    "includeRevisionCount": true,
    "includeTbrLine": true
  },
  "contacts": [
    "ynovikov@google.com"
  ],
  "gerrit": {
    "url": "https://chromium-review.googlesource.com",
    "project": "chromium/src",
    "config": "chromium"
  },
  "isInternal": false,
  "ownerPrimary": "borenet",
  "ownerSecondary": "rmistry",
  "parentDisplayName": "Chromium Beta Branch",
  "parentWaterfall": "https://build.chromium.org",
  "rollerName": "angle-chromium-beta-autoroll",
  "serviceAccount": "chromium-release-autoroll@skia-public.iam.gserviceaccount.com",
  "sheriff": [
    "https://chrome-ops-rotation-proxy.appspot.com/current/grotation:angle-wrangler"
  ],
  "sheriffBackup": [
    "ynovikov@chromium.org"
  ],
  "noCheckoutDEPSRepoManager": {
    "childBranch": "refs/heads/chromium/{{.Branches.Chromium.Beta.Number}}",
    "childPath": "src/third_party/angle",
    "childRepo": "https://chromium.googlesource.com/angle/angle.git",
    "childRevLinkTmpl": "https://chromium.googlesource.com/angle/angle.git/+show/%s",
    "parentBranch": "refs/branch-heads/{{.Branches.Chromium.Beta.Number}}",
    "parentRepo": "https://chromium.googlesource.com/chromium/src.git"
  },
  "kubernetes": {
    "cpu": "1",
    "memory": "2Gi",
    "readinessInitialDelaySeconds": "30",
    "readinessPeriodSeconds": "30",
    "readinessFailureThreshold": "10"
  }
}
