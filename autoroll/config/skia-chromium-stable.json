// See https://skia.googlesource.com/buildbot.git/+show/master/autoroll/go/roller/config.go#130
// for documentation of the autoroller config.
{
  "childDisplayName": "Skia",
  "contacts": [
    "borenet@google.com"
  ],
  "isInternal": false,
  "ownerPrimary": "borenet",
  "ownerSecondary": "rmistry",
  "parentDisplayName": "Chromium Stable Branch",
  "parentWaterfall": "https://build.chromium.org",
  "rollerName": "skia-chromium-stable",
  "serviceAccount": "chromium-autoroll@skia-public.iam.gserviceaccount.com",
  "sheriff": [
    "https://chrome-ops-rotation-proxy.appspot.com/current/grotation:skia-gardener"
  ],
  "commitMsg": {
    "bugProject": "chromium",
    "childLogURLTmpl": "https://skia.googlesource.com/skia.git/+log/{{.RollingFrom}}..{{.RollingTo}}",
    "cqExtraTrybots": [
      "luci.chromium.try:android_optional_gpu_tests_rel",
      "luci.chromium.try:linux-blink-rel",
      "luci.chromium.try:linux-chromeos-compile-dbg",
      "luci.chromium.try:linux_optional_gpu_tests_rel",
      "luci.chromium.try:mac_optional_gpu_tests_rel",
      "luci.chromium.try:win_optional_gpu_tests_rel"
    ],
    "cqDoNotCancelTrybots": true,
    "includeLog": true,
    "includeRevisionCount": true,
    "includeTbrLine": true,
    "includeTests": true,
    "template": "default"
  },
  "gerrit": {
    "url": "https://chromium-review.googlesource.com",
    "project": "chromium/src",
    "config": "chromium"
  },
  "noCheckoutDEPSRepoManager": {
    "childBranch": "refs/heads/chrome/m{{.Branches.Chromium.Stable.Milestone}}",
    "parentBranch": "refs/branch-heads/{{.Branches.Chromium.Stable.Number}}",
    "parentRepo": "https://chromium.googlesource.com/chromium/src.git",
    "gerrit": {
      "url": "https://chromium-review.googlesource.com",
      "project": "chromium/src",
      "config": "chromium"
    },
    "childRepo": "https://skia.googlesource.com/skia.git"
  },
  "kubernetes": {
    "cpu": "1",
    "memory": "2Gi",
    "readinessFailureThreshold": "10",
    "readinessInitialDelaySeconds": "30",
    "readinessPeriodSeconds": "30"
  },
  "notifiers": [
    {
      "filter": "warning",
      "email": {
        "emails": [
          "borenet@google.com"
        ]
      }
    }
  ],
  "supportsManualRolls": true
}
