      "cipd_packages": [
        {
          "name": "skia/bots/go",
          "path": "go",
          "version": "version:2"
        }
      ],
      "dimensions": [
        "pool:Skia",
        "os:Ubuntu-14.04",
        "gpu:none",
        "cpu:x86-64-avx2"
      ],
      "extra_args": [
        "--workdir",
        "../../..",
        "swarm_infra",
        "repository=<(REPO)",
        "buildername=Infra-PerCommit-Large",
        "mastername=fake-master",
        "buildnumber=2",
        "slavename=fake-buildslave",
        "nobuildbot=True",
        "swarm_out_dir=${ISOLATED_OUTDIR}",
        "revision=<(REVISION)",
        "patch_storage=<(PATCH_STORAGE)",
        "patch_issue=<(ISSUE)",
        "patch_set=<(PATCHSET)"
      ],
      "isolate": "swarm_recipe.isolate",
